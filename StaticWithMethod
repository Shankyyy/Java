package flow;

public class CC 
{
	int rollno;
    String name;
    static String college;
    
    CC(int r,String n)
    {
    	rollno=r;
    	name=n;
    }
    
    public static void change()
    {
    	college="Seamdu";
    }
    
    public void display()
    {
    	System.out.println(rollno+" "+name+" "+college);
    }
	
	public static void main(String[] args) 
	{
		CC one=new CC(111,"Shing");
		CC two=new CC(222,"Ajeet");
		one.display();
		two.display();
	}

}
